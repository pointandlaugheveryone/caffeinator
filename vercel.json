{
  "version": 2,
  "functions": {
    "api/kupi_update.py": {
      "runtime": "python3.12",
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ],
  "crons": [
    {
      "path": "/api/kupi_update",
      "schedule": "0 0 * * *"
    }
  ]
}